{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "CONTACT APP API",
    "description": "see api_link"
  },
  "paths": {
    "/contacts": {
      "get": {
        "description": "get all contacts list",
        "operationId": "getContacts",
        "responses": {
          "200": {
            "description": "success get contact list data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetContactsList"
                }
              }
            }
          },
          "401": {
            "description": "unauthorized user",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "post": {
        "description": "create contact data",
        "operationId": "createContact",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateContactRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "success create contact data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NoDataResponse"
                }
              }
            }
          },
          "400": {
            "description": "create contact error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "401": {
            "description": "unauthorized user",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/contacts/{id}": {
      "get": {
        "description": "get contact by id",
        "operationId": "getContactByID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "description": "contact id"
          }
        ],
        "responses": {
          "200": {
            "description": "success get contact by id",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Contact"
                }
              }
            }
          },
          "400": {
            "description": "error get contact by id",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "401": {
            "description": "unauthorized user",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "put": {
        "description": "update contact data by id",
        "operationId": "updateContactByID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "description": "contact id"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateContactRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "success create contact data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NoDataResponse"
                }
              }
            }
          },
          "400": {
            "description": "create contact error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "401": {
            "description": "unauthorized user",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "NoDataResponse": {
        "type": "object",
        "required": [
          "data",
          "message"
        ],
        "properties": {
          "data": {
            "type": null
          },
          "message": {
            "type": "string"
          }
        }
      },
      "ErrorResponse": {
        "type": "object",
        "required": [
          "data",
          "message"
        ],
        "properties": {
          "data": {
            "type": null
          },
          "message": {
            "type": "string"
          }
        }
      },
      "CreateContactRequest": {
        "type": "object",
        "required": [
          "name",
          "phone",
          "profilePictureURL"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "phoe": {
            "type": "string"
          },
          "profilePictureURL": {
            "type": "string"
          }
        }
      },
      "GetContactsList": {
        "type": "object",
        "required": [
          "message",
          "data"
        ],
        "properties": {
          "message": {
            "type": "string"
          },
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Contact"
            }
          }
        }
      },
      "Contact": {
        "type": "object",
        "required": [
          "id",
          "name",
          "phone",
          "profilePictureURL"
        ],
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          },
          "profilePictureURL": {
            "type": "string"
          }
        }
      },
      "UpdateContactRequest": {
        "type": "object",
        "required": [
          "name",
          "phone",
          "profilePictureURL"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          },
          "profilePictureURL": {
            "type": "string"
          }
        }
      }
    }
  }
}
